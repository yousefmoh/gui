<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_height="match_parent"
    android:layout_width="match_parent"

    xmlns:android="http://schemas.android.com/apk/res/android">
<RelativeLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <include
        layout="@layout/app_bar"
        />


<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_below="@+id/title"
    android:id="@+id/buttonslayout"
    android:layout_gravity="center"
    android:gravity="right"
    android:layout_marginTop="80dp"
    android:layoutDirection="rtl"
    android:layout_marginRight="10dp"
    android:layout_marginLeft="10dp"
    tools:context="com.example.dexter.designinsurance.MainActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:background="#f4e4ed"
        android:layout_height="wrap_content">
    <TextView
        android:layout_width="0dp"
        android:text="السيد جمال نايف براهمة"
        android:layout_weight="1"
        android:id="@+id/testText1"
        android:layout_height="wrap_content" />
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:id="@+id/testText2"

            android:text="رئيس مجلس الادارة و الرئيس التنفيذي"
            android:layout_height="wrap_content" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_marginTop="5dp"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="0dp"
            android:text="السيد نبيل جوزف قطران"
            android:layout_weight="1"
            android:id="@+id/testText3"

            android:layout_height="wrap_content" />
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:id="@+id/testText4"
            android:text="نائب رئيس مجلس الإدارة"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_marginTop="5dp"
        android:background="#f4e4ed"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="0dp"
            android:text="السيد سعيد خليل صيام"
            android:layout_weight="1"
            android:id="@+id/testText5"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:id="@+id/testText6"

            android:text="عضو مجلس إدارة"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_marginTop="5dp"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="0dp"
            android:text="السيد روبرت جنفزيان"
            android:layout_weight="1"
            android:id="@+id/testText7"

            android:layout_height="wrap_content" />
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:id="@+id/testText8"
            android:text="عضو مجلس إدارة"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:background="#f4e4ed"
        android:layout_marginTop="5dp"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="0dp"
            android:text="السيد تحسين نايف براهمة"
            android:layout_weight="1"
            android:id="@+id/testText9"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:id="@+id/testText10"
            android:text="عضو مجلس إدارة"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_marginTop="5dp"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="0dp"
            android:text="السيد لؤي شفيق خوري"
            android:layout_weight="1"
            android:id="@+id/testText11"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:id="@+id/testText12"
            android:text="عضو مجلس إدارة"
            android:layout_height="wrap_content" />
    </LinearLayout>
<LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:background="#f4e4ed"
        android:layout_marginTop="5dp"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="0dp"
            android:text="السيد جان رفائيل صليب"
            android:layout_weight="1"
            android:id="@+id/testText13"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:id="@+id/testText14"
            android:text="عضو مجلس إدارة"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_marginTop="5dp"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="0dp"
            android:id="@+id/testText15"
            android:text="الدكتور سميح حسين العبد"
            android:layout_weight="1"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:id="@+id/testText16"
            android:text="عضو مجلس إدارة"
            android:layout_height="wrap_content" />
    </LinearLayout>

</LinearLayout>
</RelativeLayout>
</ScrollView>